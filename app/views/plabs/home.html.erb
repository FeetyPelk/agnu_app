                <div class="boop"><%= label_tag "Runner on First" %><%= search_field_tag  :runner1b, value = params[:runner1b]%></div>
                <div class="boop"><%= label_tag "Runner on Second" %><%= search_field_tag  :runner2b, value = params[:runner2b]%></div>
                <div class="boop"><%= label_tag "Runner on Third" %><%= search_field_tag  :runner3b, value = params[:runner3b]%></div>
                </p>
              </div>

            </div>
          </div>
        </div>
