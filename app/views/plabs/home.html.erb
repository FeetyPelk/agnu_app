                <div class="boop"><%= label_tag "Shortstop" %><%= search_field_tag  :shortstop, value = params[:shortstop]%></div>
                <div class="boop"><%= label_tag "Left Field" %><%= search_field_tag  :left_field, value = params[:left_field]%></div>
                <div class="boop"><%= label_tag "Center Field" %><%= search_field_tag  :center_field, value = params[:center_field]%></div>
                <div class="boop"><%= label_tag "Right Field" %><%= search_field_tag  :right_field, value = params[:right_field]%></div>
                </p>
              </div>
            </div>
          </div>
        </div>
        <div class="accordion-group">
          <div class="accordion-heading">
            <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseThree">
              Dates
            </a>
          </div>
          <div id="collapseThree" class="accordion-body collapse">
            <div class="accordion-inner">
              <div class="row">
                <p>
                <div class="boop"><%= label_tag "Start Date" %><%= search_field_tag  :start_date, value = params[:start_date]%></div>
                <div class="boop"><%= label_tag "End Date" %><%= search_field_tag  :end_date, value = params[:end_date]%></div>
                </p>
              </div>
            </div>
          </div>
        </div>

        <!--   This is a comment   -->




        <div class="accordion-group">
          <div class="accordion-heading">
            <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseFour">
              Grouping
            </a>
          </div>
          <div id="collapseFour" class="accordion-body collapse">
            <div class="accordion-inner">
              <p> Group by: </p>
