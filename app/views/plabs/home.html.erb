      <p>
        <%= submit_tag "Submitto", class: "btn btn-large btn-primary"%>

      </p>



      <table class="footable">
        <thead>
        <!--
        <th><%= link_to "TField", :sort => "tdf.id3_old"  if $fteam_colm%></th>
        <th><%= link_to 'TField' do %> <i class: "btn btn-large btn-primary"</i> if $fteam_colm% <% end %> </th>
     -->

        <tr>
          <th><%= link_to "Year", :sort => "dd.calendar_year"  if $date_year_colm%></span></th>
          <th><%= link_to "Month", :sort => "dd.calendar_yearmonth"  if $date_month_colm%></th>
          <th data-class="expand">Batter </th>
          <th><%= link_to "H/A", :sort => "at_home"  if $home_colm%></th>
          <th><%= link_to "TBat", :sort => "tdb.id3_old"  if $bteam_colm%></th>
          <th><%= link_to "TField", :sort => "tdf.id3_old" if $fteam_colm%></th>
          <th> Avg</th>
          <th data-hide="phone"> OBP</th>
          <th data-hide="phone"> SLG</th>
          <th data-hide="phone"> OPS</th>
          <th data-hide="phone"> ERA</th>
          <th data-hide="phone">  Atbat</th>
          <th data-hide="phone"> Hit</th>
          <th data-hide="phone,tablet"> Walk</th>
          <th data-hide="phone,tablet"> RBI</th>
          <th data-hide="phone,tablet"> ippies</th>
          <th data-hide="phone,tablet"> earned_runs</th>
          <th><%=  link_to "Pitcher", :sort => "fd.pitcher"  if $pitcher_colm%></th>
          <th><%= link_to  "Catcher", :sort => "fd.catcher"  if $catcher_colm%></th>
          <th><%= link_to  "First Base", :sort => "fd.first_base"  if $firstb_colm%></th>
          <th><%=  link_to "Second Base", :sort => "fd.second_base"  if $secondb_colm%></th>
          <th><%=  link_to "Shortstoppy", :sort => "fd.shortstop"  if $ss_colm%></th>
          <th><%=  link_to "Third Base", :sort => "fd.third_base"  if $thirdb_colm%></th>
          <th><%=  link_to "Left Field", :sort => "fd.left_field"  if $lf_colm%></th>
          <th><%= link_to  "Center Field", :sort => "fd.center_field"  if $cf_colm%></th>
          <th><%=  link_to "Right Field", :sort => "fd.right_field"  if $rf_colm%></th>
          <th><%=  link_to "Runner 1b", :sort => "fd.runner_1b"  if $runner1b_colm%></th>
          <th><%=  link_to "Runner 2b", :sort => "fd.runner_2b"  if $runner2b_colm%></th>
          <th><%=  link_to "Runner 3b", :sort => "fd.runner_3b"  if $runner3b_colm%></th>

        </tr>
        </thead>

        <div id="tabby">
          <tbody>
          <% @Plabs.each do |plab| %>

              <tr>
                <td><%= plab.yearo if $date_year_colm%></td>
                <td><%= plab.montho if $date_month_colm%></td>
                <td><%= plab.batter if $batter_colm%></td>
                <td><%= plab.at_home if $home_colm%></td>
                <td><%= plab.bteam if $bteam_colm%></td>
                <td><%= plab.fteam if $fteam_colm%></td>
                <td><%= plab.avg%></td>
                <td><%= plab.obp%></td>
                <td><%= plab.slg%></td>
                <td><%= plab.ops%></td>
                <td><%= plab.era%></td>
                <td><%= plab.at_bat%></td>
                <td><%= plab.hit%></td>
                <td><%= plab.walk%></td>
                <td><%= plab.rbi%></td>
                <td><%= plab.ippies%></td>
                <td><%= plab.earned_runs%></td>
                <td><%= plab.pitcher if $pitcher_colm%></td>
                <td><%= plab.catcher if $catcher_colm%></td>
                <td><%= plab.first_base if $firstb_colm%></td>
                <td><%= plab.second_base if $secondb_colm%></td>
                <td><%= plab.shortstop if $ss_colm%></td>
                <td><%= plab.third_base if $thirdb_colm%></td>
                <td><%= plab.left_field if $lf_colm%></td>
                <td><%= plab.center_field if $cf_colm%></td>
                <td><%= plab.right_field if $rf_colm%></td>
                <td><%= plab.runner1b if $runner1b_colm%></td>
                <td><%= plab.runner2b if $runner2b_colm%></td>
                <td><%= plab.runner3b if $runner3b_colm%></td>
              </tr>
          <% end unless @Plabs.nil?%>
          </tbody>
        </div>
